<div id="show_container" class="form_container">
  <div class ="headline">
   Leave Detail
  </div>
  
  
  <div class="l_row">
    <span class="l_label">
      Name:<span class="mandatory"></span>
    </span>
    <span class="r_label">
      <%= @leave.leave_type.name  %>
    </span>
  </div>
  <div class="l_row">
    <span class="l_label">
      Start Date:<span class="mandatory"></span>
    </span>
    <span class="r_label">
      <%= @leave.start_date  %>
    </span>
  </div>
  <div class="l_row">
    <span class="l_label">
      End Date:<span class="mandatory"></span>
    </span>
    <span class="r_label">
      <%= @leave.end_date  %>
    </span>
  </div>
 <div class="l_row">
    <span class="l_label">
      Image:<span class="mandatory"></span>
    </span>
    <span class="r_label">
      <%= @leave.image  %>
    </span>
  </div>


  
</div>
